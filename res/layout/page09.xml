<?xml version="1.0" encoding="utf-8"?>

<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              android:id="@+id/layout_p09"
        >

    <andr.lexibook.mylittlestory.lrrh.ui.ViewIml.GifMovieView
            android:id="@+id/gif_p09_basket"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_x="@dimen/p09_basket_x"
            android:layout_y="@dimen/p09_basket_y"
            />
    <andr.lexibook.mylittlestory.lrrh.ui.ViewIml.GifMovieView
            android:id="@+id/gif_p09_hunter"
            android:layout_height="fill_parent"
            android:layout_width="fill_parent"
            />
    <andr.lexibook.mylittlestory.lrrh.ui.ViewIml.GifMovieView
            android:id="@+id/gif_p09_wolf"
            android:layout_height="fill_parent"
            android:layout_width="fill_parent"
            />

    <AbsoluteLayout
            android:id="@+id/al_pause_p09"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_x="@dimen/btn_play_pause_p09_x"
            android:layout_y="@dimen/btn_play_pause_p09_y"
            android:background="@drawable/lrrh_pasue"
            android:visibility="gone"></AbsoluteLayout>
</AbsoluteLayout>